                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/f_o_register_establishment_photo_btn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:src="@drawable/ic_image_search"
                    app:layout_constraintBottom_toBottomOf="@+id/f_o_register_establishment_photo"
                    app:layout_constraintEnd_toEndOf="@+id/f_o_register_establishment_photo"
                    app:layout_constraintStart_toStartOf="@+id/f_o_register_establishment_photo"
                    app:layout_constraintTop_toTopOf="@+id/f_o_register_establishment_photo" />
            </androidx.constraintlayout.widget.ConstraintLayout>

